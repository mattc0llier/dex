{
  "manifest_version": 2,
  "name": "DEX",
  "description": "A simple application to track live cryptocurrency prices",
  "version": "1.0",
  "permissions": [
    "tabs",
    "bookmarks",
    "unlimitedStorage",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
     "default_icon": "calendar-small.png",
     "default_title": "DEX"
  }
}
